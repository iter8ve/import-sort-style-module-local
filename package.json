{
  "name": "import-sort-style-module-local",
  "version": "1.0.0",
  "description": "An import-sort style that groups and sorts by module, with support for local files.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc -p src",
    "build:watch": "tsc -p src -w",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.ts\"",
    "test:watch": "mocha -w --require ts-node/register --recursive \"test/**/*.ts\""
  },
  "author": "Michael Bourke (michael@iter8ve.com)",
  "repository": "iter8ve/import-sort-style-module-local",
  "license": "ISC",
  "dependencies": {
    "import-sort-style": "^6.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.31",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "ts-node": "^10.7.0",
    "tslint": "^5.20.1",
    "typescript": "^4.6.4"
  }
}
